{
  "name": "vms-nest-prisma-api-document",
  "version": "1.2.1",
  "description": "Reusable API SDK built with NestJS, Prisma, Axios, and Zod for VMS frontends.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "repository": "https://github.com/ramkitech/vms_nest_prisma_api_document",
  "bugs": "https://github.com/ramkitech/vms_nest_prisma_api_document/issues",
  "homepage": "https://github.com/ramkitech/vms_nest_prisma_api_document#readme",
  "keywords": [
    "nestjs",
    "prisma",
    "sdk",
    "api",
    "axios",
    "zod",
    "typescript",
    "frontend",
    "react",
    "nextjs"
  ],
  "author": "RamkiTech",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./services/*": {
      "import": "./dist/services/*",
      "require": "./dist/services/*"
    },
    "./core/*": {
      "import": "./dist/core/*",
      "require": "./dist/core/*"
    },
    "./zod/*": {
      "import": "./dist/zod/*",
      "require": "./dist/zod/*"
    }
  },
  "typesVersions": {
    "*": {
      "services/*": [
        "dist/services/*"
      ],
      "core/*": [
        "dist/core/*"
      ],
      "zod/*": [
        "dist/zod/*"
      ]
    }
  },
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.11.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.4.3",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
